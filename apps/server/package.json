{
  "name": "@s2media/server",
  "version": "1.0.0",
  "description": "server for s2media",
  "main": "src/server.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon",
    "start": "yarn prisma:migrate:deploy && cross-env NODE_ENV=production node dist/src/server.js",
    "build": "rimraf ./dist && tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettify": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "clean": "rm -rf ./node_modules",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:dev": "prisma migrate dev",
    "docker:start": "yarn prisma:migrate:dev && yarn prisma:generate && yarn dev"
  },
  "keywords": [
    "express",
    "postgres",
    "prisma",
    "node",
    "typescript"
  ],
  "author": "sairaj2119",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "cloudinary": "^1.28.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "datauri": "^3.0.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-handler": "^1.2.0",
    "express-validator": "^6.12.0",
    "firebase-admin": "^9.11.0",
    "http-errors": "^1.8.0",
    "ioredis": "^4.28.5",
    "multer": "^1.4.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.9",
    "@types/http-errors": "^1.8.0",
    "@types/ioredis": "^4.28.1",
    "@types/morgan": "^1.9.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^15.12.5",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "prisma": "^3.10.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2",
    "uuid": "^8.3.2"
  }
}
