{
  "name": "s2media",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": "sairaj2119",
  "keywords": [],
  "description": "A social media website",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sairaj2119/s2media.git"
  },
  "bugs": {
    "url": "https://github.com/sairaj2119/s2media/issues"
  },
  "homepage": "https://github.com/sairaj2119/s2media#readme",
  "engines": {
    "node": ">=14",
    "yarn": ">=1.22.0",
    "npm": "use-yarn-instead"
  },
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:client": "yarn workspace @s2media/client run dev",
    "dev:server": "yarn workspace @s2media/server run dev",
    "dev": "npm-run-all --parallel dev:* ",
    "lint:client": "yarn workspace @s2media/client run lint",
    "lint:server": "yarn workspace @s2media/server run lint",
    "lint": "npm-run-all lint:*",
    "studio:server": "yarn workspace @s2media/server run prisma:studio",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "npm-run-all": "^4.1.5"
  }
}
